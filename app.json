{
  "expo": {
    "name": "Currency Converter",
    "slug": "currency-converter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconCC.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#EDE9FE"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-iconCC.png",
        "backgroundColor": "#EDE9FE"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
